{
  "defaultSeverity": "warning",
  "extends": [
      "tslint:recommended",
      "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ false ],
      "ordered-imports": [ false ],
      "object-literal-sort-keys": [ false ],
      "no-var-requires": [ false ],
      "no-floating-promises": [ false ],
      "interface-over-type-literal": [ false ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.{ts,tsx,js}",
      "node_modules/**/*",
      "coverage/**/*.{js,ts,tsx}"
    ]
  }
}